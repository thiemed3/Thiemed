<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="account_line_inherit_analytic_form" model="ir.ui.view">
            <field name="name">account_line_inherit_analytic_form</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_form"/>
            <field name="priority" eval="8"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='price_total']" position="after">
                    <field name="cantidad_lote" readonly="0" optional="show"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>